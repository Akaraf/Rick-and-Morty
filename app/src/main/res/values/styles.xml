<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="StartEndMargin16">
    <item name="android:layout_marginRight">16dp</item>
    <item name="android:layout_marginLeft">16dp</item>
    </style>
    <style name="StartEndMargin8">
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
    </style>
    <style name="AllMargin8">
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>
    <style name="AllMargin16">
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginBottom">16dp</item>
    </style>
    <style name="CustomShapeImageView">
        <item name="shapeAppearanceOverlay">@style/RoundedBottomCorners</item>
    </style>
    <style name="RoundedBottomCorners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomLeft">24dp</item>
        <item name="cornerSizeBottomRight">24dp</item>
    </style>
</resources>